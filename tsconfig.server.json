{
  "compilerOptions": {
    "outDir": "dist",
    "noEmit": false,
    "module": "ESNext",
    "target": "ES6",
    "rootDir": "./",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "typeRoots": ["./node_modules/@types"],
    "types": ["node", "express"]
  },
  "include": ["server.ts"],
  "exclude": ["node_modules", "dist"]
}
